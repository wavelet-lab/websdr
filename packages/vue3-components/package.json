{
    "name": "@websdr/vue3-components",
    "version": "1.0.0",
    "description": "This is a Vue 3 components package for WebSDR",
    "author": "Timur <dtv.comp@gmail.com>",
    "license": "MIT",
    "keywords": [],
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "vue-tsc && tsc-alias && tsc-esm-fix && vite build && npm run build:styles && npm run build:types",
        "build:styles": "node scripts/build-styles.js",
        "build:types": "vue-tsc --emitDeclarationOnly",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@websdr/core": "^1.0.0",
        "@websdr/frontend-core": "^1.0.0",
        "vue": "^3.5.23"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/cli-plugin-typescript": "^5.0.9",
        "@vue/cli-service": "^5.0.9",
        "@vue/test-utils": "^2.4.6",
        "happy-dom": "^20.0.10",
        "sass": "^1.93.3",
        "vue-tsc": "^3.1.3"
    },
    "sideEffects": [
        "**/*.css",
        "**/*.scss",
        "**/*.sass"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        },
        "./styles/*": "./dist/styles/*"
    }
}